{
  "name": "Google Tasks Fixer",
  "description" : "Works around Google Tasks not working as a standalone app",
  "homepage_url": "https://github.com/kallepersson/googletasks-standalone",
  "author": "kallepersson",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://tasks.google.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
